    "ts-sql-query": "^1.67.0",
    "twilio": "^5.7.3",
    "uuid": "^11.1.0",
    "zod": "^4.0.5"
  },
  "overrides": {
    "nodemailer-mock": {
      "nodemailer": "^6.x || ^7.x"
    },
    "react-hook-form-mui": {
      "@mui/x-date-pickers": "^8.2.0"
    }
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.4.3",
    "@playwright/test": "^1.54.1",
    "@simplewebauthn/typescript-types": "^8.3.4",
