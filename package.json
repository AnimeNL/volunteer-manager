    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jest-extended": "^6.0.0",
    "next-build-id": "^3.0.0",
    "nodemailer-mock": "^2.0.9",
    "ts-sql-codegen": "^3.23.1",
    "typescript": "^5.8.3",
    "whatwg-fetch": "^3.6.20"
  }
